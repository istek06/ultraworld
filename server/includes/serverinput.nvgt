class serverinput {
	string title, packet, text, button1, button2, default_text;
	int maxlength = 0, readonly = 0, multiline = 0;
	int button1primary = 0, button1cancel = 0, button2primary = 0, button2cancel = 0;
	int type = 0;
	serverinput() {
		this.reset();
	}
	void reset() {
		this.title = "Test";
		this.type = 0;
		this.text = "Enter text";
		this.packet = "testmenu";
		this.button1 = "Ok";
		this.button2 = "Cancel";
		this.maxlength = 0;
		this.readonly = 0;
		this.multiline = 0;
		this.default_text = "";
		this.button1primary = 1;
		this.button1cancel = 0;
		this.button2primary = 0;
		this.button2cancel = 1;
	}
	void send(uint id) {
		messager a;
		a.add("message", "serverinput");
		a.add("title", this.title);
		a.add("type", this.type);
		a.add("inptext", this.text);
		a.add("default_text", default_text);
		a.add("packet", this.packet);
		a.add("length", this.maxlength);
		a.add("readonly", this.readonly);
		a.add("multiline", this.multiline);
		a.add("button1", this.button1);
		a.add("button2", this.button2);
		a.add("button1primary", this.button1primary);
		a.add("button1cancel", this.button1cancel);
		a.add("button2primary", this.button2primary);
		a.add("button2cancel", this.button2cancel);
		::send(id, a, 0, true);
	}//End.send
}//End