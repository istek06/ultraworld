void spawn_weapon(double x, double y, double z, double dir, double ddir, string type, player@p, mapdata@m) {
	weapon w1(x, y, z, dir, ddir, type, p, m);
	if (@w1.map == null or @w1.owner == null) return;
	else weapons.insert_last(w1);
}
