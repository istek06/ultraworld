void spawn_grenade(int x, int y, int z, mapdata@m, player@p) {
	grenade bomb1(x, y, z, m, p);
	if (@bomb1.map == null or @bomb1.owner == null) return;
	else grenades.insert_last(bomb1);
}
void destroy_all_grenades() {
	grenades.resize(0);
}
