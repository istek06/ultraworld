			for (uint i2 = 0; i2 < air_conditioners.length(); i2++) {
				if (owner.pvp == 1 and air_conditioners[i2].map == map.name and get_3d_distance(x, y, z, air_conditioners[i2].x, air_conditioners[i2].y, air_conditioners[i2].z) <= 30) {
