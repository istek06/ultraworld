				else if (string_to_number(parsed[1]) < 1000)send_reliable(p, "ttschat you've sent a " + ms_to_readable_time(string_to_number(parsed[1])) + " private tts chat to " + players[i].name + " on " + get_date() + " at " + get_time() + "", 0);
