		if (string_to_number(parsed[1]) == 1) send_packet(0, "ttschat " + p.name + " has sent a " + string_to_number(parsed[1]) + " character tts chat on " + get_date() + " at " + get_time() + "", p.x, p.y, p.z, maps[get_map_index(p.map)]);
